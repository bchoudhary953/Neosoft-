<script>

function login()
{
var gurl='';

$.ajaxSetup({
             crossDomain: true,
             xhrFields: {
                 withCredentials: true
             }
         });

		$.ajax({

		//
//url: "dynamic/dashboard.html",
//			url: ((typeof gurl !== 'undefined')?gurl:"http://server.dslroot.com/?&UserID=3&tplsTotal=1&tpl[1]=tpl_new_content_list2&new_site=1"),
			//url: (( gurl != '')?gurl:document.location.origin +'/'+"/?&UserID=3&tplsTotal=1&tpl[1]=tpl_new_content_list2&new_site=1"),
			url: (( gurl != '')?gurl:'http://dashboard.dslroot.com' +'/'+"?UserID="+$("#pi-email-id").val()+"&tplsTotal=1&tpl[1]=tpl_new_content_list2&new_site=1"),
type: "POST"			, 
data: "password="+$("#pi-pas-id").val(),


			context: document.body,
			success: function(response) {

//				$( "#dynamic-content" ).html(response);
				//$( "#dynamic-headercontent" ).html(response);
//[0].outerHTML
//accinf=($(response).find("#my-account-page").outerHtml());
  //alert($(response).find("#user__info-id").html());
//if ($(response).find("#nologin").html()!='1')

if ($(response).find(".user__info-id").html()!==undefined)
  {
  window.location='http://dashboard.dslroot.com/';
  }
  else
  {
	$('#log-msg').html('<font color=red>Invalid login data</font>');
	}

			}
		});
}
</script>    <!-- Footer Section End -->
